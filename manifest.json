{
  "name": "Invisalign scan exporter",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Export your invisalign scan to a .obj file!",
  "homepage_url": "https://github.com/ferminLR/invisalign-scan-exporter",
  "background": {
    "service_worker": "extension.js"
  },
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
      "*://share.invisalign.com/treatment-viewer/*"
  ],
  "web_accessible_resources": [{
    "resources": ["scanExporter.js"],
    "matches": ["<all_urls>"]
  }]
}